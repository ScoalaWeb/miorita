{
    "name": "miorita",
    "version": "2.1.0",
    "private": true,
    "scripts": {
        "dev": "nuxt",
        "editor": "nuxt --config-file editor.config.js",
        "build": "nuxt build",
        "start": "nuxt start",
        "generate": "nuxt generate",
        "lint:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
        "lint-fix:js": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
        "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
        "lint-fix:style": "stylelint \"**/*.{vue,css,scss}\" --fix",
        "lint": "yarn lint:js && yarn lint:style",
        "cypress": "cypress open",
        "cypress:e2e": "cypress open --e2e",
        "test:e2e": "./node_modules/.bin/cypress run --e2e",
        "prepare": "husky install && npx patch-package"
    },
    "lint-staged": {
        "*.{js,vue}": "eslint",
        "*.{css,vue}": "stylelint"
    },
    "dependencies": {
        "@actions/cache": "^3.1.2",
        "@actions/core": "^1.10.0",
        "@nuxt/content": "^1",
        "@nuxtjs/composition-api": "^0.33.1",
        "@nuxtjs/i18n": "^7.3.1",
        "@nuxtjs/pwa": "^3.3.5",
        "@pinia/nuxt": "0.2.1",
        "core-js": "^3.27.1",
        "debounce": "^1.2.1",
        "howler": "^2.2.3",
        "keen-slider": "^6.8.5",
        "nuxt": "^2.15.8",
        "pinia": "^2.0.29",
        "postcss": "^8.4.21",
        "postcss-scss": "^4.0.6",
        "postinstall": "^0.8.0",
        "sass": "^1.57.1",
        "stylelint-config-standard-scss": "^6.1.0",
        "vue": "^2.7.11",
        "vue-class-component": "^7.2.6",
        "vue-easy-dnd": "^1.22.0",
        "vue-monaco": "^1.2.2",
        "vue-property-decorator": "^9.1.2",
        "webpack": "^5.75.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.2",
        "@commitlint/config-conventional": "^17.4.2",
        "@nuxt/types": "^2.15.8",
        "@nuxt/typescript-build": "^2.1.0",
        "@nuxtjs/color-mode": "^3.2.0",
        "@nuxtjs/eslint-config": "^12.0.0",
        "@nuxtjs/eslint-config-typescript": "^12.0.0",
        "@nuxtjs/eslint-module": "^3.1.0",
        "@nuxtjs/stylelint-module": "^4.1.0",
        "@nuxtjs/svg": "^0.4.0",
        "@types/howler": "^2.2.7",
        "babel-eslint": "^10.1.0",
        "body-parser": "^1.20.1",
        "cypress": "^12.4.1",
        "eslint": "^8.32.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-editorconfig": "^4.0.2",
        "eslint-plugin-nuxt": "^4.0.0",
        "eslint-plugin-vue": "^9.9.0",
        "express": "^4.18.2",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.0",
        "patch-package": "^6.5.1",
        "stylelint": "^14.16.1",
        "stylelint-config-prettier": "^9.0.4",
        "stylelint-config-standard": "^29.0.0",
        "stylelint-processor-html": "^1.0.0"
    }
}
